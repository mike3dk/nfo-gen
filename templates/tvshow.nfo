<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<tvshow>
  <title>{{ title }}</title>
  <uniqueid type="learn" default="true"></uniqueid>
  <year>{{ year }}</year>
  <premiered>{{ year }}</premiered>
  <director>{{ author }}</director>
  <studio>{{ source }}</studio>
  {% for tag in tags %}
  <genre>{{ tag }}</genre>
  {% endfor %}
  <tag>{{ tag }}</tag>
  <url>{{ url }}</url>
  <thumb>
    {{ image }}
  </thumb>
</tvshow>
